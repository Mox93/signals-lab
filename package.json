{
  "name": "signals-lab",
  "version": "0.0.0",
  "description": "A playground for learning how signals are implemented",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/* --format cjs,esm --dts",
    "dev": "tsup src/* --format cjs,esm --dts --watch",
    "bench": "pnpm build && node --jitless --expose-gc benchs/propagate.mjs",
    "memory": "pnpm build && node --expose-gc benchs/memoryUsage.mjs",
    "compare": "pnpm build && node --jitless --expose-gc benchs/compare.mjs"
  },
  "keywords": [
    "signals",
    "reactivity"
  ],
  "author": "Mohamed Ragaiy Saleh",
  "license": "MIT",
  "packageManager": "pnpm@10.11.0",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.21",
    "jest": "^29.7.0",
    "mitata": "^1.0.34",
    "ts-jest": "^29.3.2",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ],
    "ignoredBuiltDependencies": [
      "esbuild"
    ]
  }
}
